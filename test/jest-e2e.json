{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e.spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFilesAfterEnv": ["./setup-e2e.ts"],
  "globalSetup": "./global-setup.ts",
  "globalTeardown": "./global-teardown.ts",
  "testTimeout": 100000,
  "maxWorkers": 1,
  "forceExit": true,
  "detectOpenHandles": true,
  "cache": false
}
